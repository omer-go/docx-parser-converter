{
  "name": "docx-parser-converter-js",
  "version": "1.0.0",
  "description": "High-performance TypeScript library for converting DOCX documents to HTML and TXT in browsers",
  "type": "module",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src test --ext .ts,.js",
    "lint:fix": "eslint src test --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "analyze": "node scripts/performance-analysis.js",
    "generate-outputs": "node scripts/generate-test-outputs.js"
  },
  "keywords": [
    "docx",
    "parser",
    "converter",
    "html",
    "txt",
    "typescript",
    "browser",
    "office",
    "word",
    "document"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/docx-parser-converter-js.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/docx-parser-converter-js/issues"
  },
  "homepage": "https://github.com/yourusername/docx-parser-converter-js#readme",
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.0",
    "jszip": "^3.10.1",
    "yauzl": "^3.2.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
